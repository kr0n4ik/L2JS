<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence>
				<xs:element type="xs:short" name="onePieceFactor" />
				<xs:element name="enchantHP" minOccurs="0" maxOccurs="12">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:short" name="bonus" minOccurs="1" maxOccurs="20" />
						</xs:sequence>
						<xs:attribute type="xs:token" name="grade" use="required" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>